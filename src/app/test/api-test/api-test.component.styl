.wrapper
  display flex
  justify-content center
  align-items center
  flex-direction column

.buttons
  display flex
  justify-content row
  gap 20px
  padding 20px
  height 30%

.console
  display block
  padding 20px
  gap 20px
  flex-direction column
  border 2px solid green
  height 70%
  width 80%

table
  font-family arial, sans-serif
  border-collapse collapse
  width 100%
  margin-top 20px

td
th
  border 1px solid #dddddd
  text-align left
  padding 8px

tr:nth-child(even)
  background-color #dddddd